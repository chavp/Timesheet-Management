@model IList<PJ_CWN019.TM.Web.Models.EmployeeView>

@{
    ViewBag.Title = "Support / Employees";
}

@Html.Partial("_Template", new PJ_CWN019.TM.Web.Models.TemplateView
    {
        MainPanelID = "employeesPanel",
        MainIcon = "glyphicon glyphicon-cog",
        MainTitle = "Support",
        SubIcon = "glyphicon glyphicon-user",
        SubTitle = "Reset Password"
    })

@section scripts{
    <script>
        Ext.Loader.setPath('Ext.ux', '@Url.Content("~/Scripts/ext-4.2.1-gpl/ux")');

        Ext.require(['Ext.ux.form.SearchField']);

        paramsView.urlReadEmployee = '@Url.Content("~/Support/GetEmployees")';
        paramsView.urlResetPassword = '@Url.Content("~/Account/ResetPassword")';
    </script>
    <script src="@Url.Content("~/app/onReady/Support/Employees.js")"></script>
}